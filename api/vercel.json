{
    "version": 2,
    "functions": {
      "api/index.php": {
        "runtime": "vercel-php@0.6.0"
      }
    },
    "routes": [
      {
        "src": "/(.*)",
        "dest": "/api/index.php"
      }
    ],
    "env": {
      "APP_ENV": "production",
      "APP_DEBUG": "true",
      "APP_URL": "https://tu-url-de-vercel.vercel.app",
      "APP_KEY": "clave-de-tu-aplicacion"
    }
  }
